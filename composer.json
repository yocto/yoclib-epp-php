{
  "name": "yocto/yoclib-epp",
  "type": "library",
  "description": "This yocLibrary enables your project to send and receive with EPP (Extensible Provisioning Protocol) in PHP.",
  "keywords":[
	"composer",
	"epp",
	"php",
	"xml",
	"yocto",
	"yoclib"
  ],
  "license": "GPL-3.0-or-later",
  "require": {
	"php": "^7.1||^8",
    "wikimedia/idle-dom": "^0.10.0",
    "wikimedia/dodo": "^0.4.0"
  },
  "require-dev": {
	"phpunit/phpunit": "^9"
  },
  "autoload": {
	"psr-4": {
	  "YOCLIB\\EPP\\": "src/"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "YOCLIB\\EPP\\Tests\\": "tests/"
	}
  }
}
